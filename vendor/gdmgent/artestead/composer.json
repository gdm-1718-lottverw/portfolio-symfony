{
    "name": "gdmgent/artestead",
    "description": "Artestead is a fork of Laravel Homestead for use at Artevelde University College Ghent.",
    "require": {
        "php": ">=7.1",
        "symfony/console": "~3.0",
        "symfony/process": "~3.0",
        "symfony/yaml": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Parent"
        }
    ],
    "autoload": {
        "psr-4": {
            "GdmGent\\Artestead\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        }
    },
    "bin": [
        "bin/artestead"
    ]
}
